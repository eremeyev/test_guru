<%= form_for [@user, @test] do |f| %>
  <%= f.label :title %>:<br/> 
  <%= f.text_field :title %><br/><br/>
  <%= f.label :level %>:<br/>
  <%= f.number_field :level %><br/><br/>
  <%= f.label :category %>:<br/>
  <%= f.collection_select :category_id, Category.all, :id, :title, {prompt: true}, {id: 'test_category'} %><br/><br/>
  <%= f.submit %>
<% end %>